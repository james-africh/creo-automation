                                                                            <% stdNamplates.push(row.part.slice(0,15)) %>
                                                                        <% } %>
                                                                    <% } %>
                                                                <% } %>

                                                                <% for (var i = 0; i < sortedCheckedDwgs.length; i++) { %>
