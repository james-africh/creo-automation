                                                    </div>
                                                    <div class="col-sm-3">
                                                        <label for="" class="col-form-label">Customer</label>
                                                        <input type="text" name="customer" class="form-control" value="<%= mbomData.customer %>" placeholder="Type Customer Here" id="customer" readonly="readonly">
