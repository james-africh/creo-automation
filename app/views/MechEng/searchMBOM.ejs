                                                        <input type="text" name="boardDesignation" class="form-control" value="<%= mbomData.boardDesignation %>" placeholder="Type Designation Here" id="boardDesignation" readonly="readonly">
                                                    </div>
                                                </div>
                                            <% } %>
                                        </div>
                                    </form>
