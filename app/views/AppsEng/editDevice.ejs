    <div class="content-page">
        <!-- Content Start -->
        <div class="content">
            <div class="container-fluid">
                <div class="row">
                    <div class="col-sm-12">
                        <div class="page-title-box">
                            <h4 class="page-title">Applications Engineering</h4>
                            <div class="row justify-content-left">
                                <div class="col-sm-12">
                                    <div class="card m-b-20">
                                        <div class="card-body">
                                            <h4 class="mt-0 header-title">Current Quote</h4>
                                                <div class="form-group">
                                                    <div class="row">
                                                        <div class="col-sm-4">
                                                            <label for="example-text-input" class="col-form-label">Quote Number</label>
                                                            <% if (quoteData) { %>
                                                            <input type="text" name="quoteNum" class="form-control" value="<%=quoteData[0].quoteNum%>" placeholder="Type Quote Number Here" id="example-text-input">
                                                            <% } %>
                                                        </div>
                                                        <div class="col-sm-4">
                                                            <label>Project Name</label>
                                                            <% if (quoteData) { %>
                                                            <input type="text" name="projName" class="form-control" value="<%=quoteData[0].projName%>" placeholder="Type Project Name Here" id="example-text-input">
                                                            <% } %>
                                                        </div>
                                                        <div class="col-sm-4">
                                                            <div class="form-group">
                                                                <label for="example-text-input" class="col-form-label">Customer</label>
                                                                <% if (quoteData) { %>
                                                                <input type="text" name="customer" class="form-control" value="<%=quoteData[0].customer%>" placeholder="Type Customer Name Here" id="example-text-input">
                                                                <% } %>
                                                            </div>
                                                        </div>
                                                    </div>
                                                </div>
                                        </div>
                                    </div>
                                </div>
                            </div>

                            <div class="row">
                                <div class="col-lg-12">
                                    <div class="card m-b-20">
                                        <div class="card-body">

                                            <h4 class="mt-0 header-title">Quote Data</h4>
                                            <!-- Nav tabs -->
                                            <ul class="nav nav-tabs nav-tabs-custom nav-justified" role="tablist">
                                                <li class="nav-item">
                                                    <a class="nav-link disabled" data-toggle="tab" href="#lineup" role="tab">Lineups</a>
                                                </li>
                                                <li class="nav-item">
                                                    <a class="nav-link active" data-toggle="tab" href="#devices" role="tab">Devices</a>
                                                </li>
                                                <li class="nav-item">
                                                    <a class="nav-link disabled" data-toggle="tab" href="#sections" role="tab">Sections</a>
                                                </li>
                                                <li class="nav-item">
                                                    <a class="nav-link disabled" data-toggle="tab" href="#hours" role="tab">Hours</a>
                                                </li>
                                            </ul>

                                            <!-- Tab panes -->
                                            <div class="tab-content">
                                                <div class="tab-pane p-3" id="lineup" role="tabpanel"></div>
                                                <div class="tab-pane active p-3" id="devices" role="tabpanel">
                                                    <p class="mb-3">
                                                        Fill out the following for each device, and click "Submit" afterwards
                                                    </p>

                                                    <form id="breakerForm" class="breakerForm" action="../saveDevice/?quoteID=<%= deviceData[0].quoteID %>&layoutNum=<%=deviceData[0].layoutNum%>&devID=<%= deviceData[0].devID%>" method="POST">
                                                        <div class="row">
                                                            <div class="col-sm-2">
                                                                <div class="form-group">
                                                                    <label for="example-text-input" class="col-form-label">Quote Number</label>
                                                                    <% if (quoteData) { %>
                                                                    <input type="text" name="quoteID" class="form-control" value="<%=quoteData[0].quoteNum%>" placeholder="Type Quote Number Here" id="example-text-input" readonly="readonly">
                                                                    <% } %>
                                                                </div>
                                                            </div>
                                                            <div class="col-sm-1">
                                                                <div class="form-group">
                                                                    <label for="example-text-input" class="col-form-label">Rev #</label>
                                                                    <% if (quoteData) { %>
                                                                    <input type="text" name="revNum" class="form-control" value="<%=quoteData[0].revNum%>"  id="example-text-input" readonly="readonly">
                                                                    <% } %>
                                                                </div>
                                                            </div>
                                                            <div class="col-sm-7">
                                                                <div class="form-group">
                                                                    <label for="example-text-input" class="col-form-label">Device Designation</label>
                                                                    <input type="text" name="devDesignation" class="form-control" value="<%= deviceData[0].devDesignation %>" placeholder="Type Quote Number Here" autocomplete="off"/>
                                                                </div>
                                                            </div>
                                                            <div class="col-sm-2">
                                                                <div class="form-group">
                                                                    <label class="control-label">Application</label>
                                                                    <select class="form-control" name="devApp">
                                                                        <option>Select</option>
                                                                        <% if(deviceData[0].devApp == "MAIN") { %>
                                                                            <option value="MAIN" selected="selected">MAIN</option>
                                                                        <% } else { %>
                                                                            <option value="MAIN">MAIN</option>
                                                                        <% } %>
                                                                        <% if(deviceData[0].devApp == "TIE") { %>
                                                                            <option value="TIE" selected="selected">TIE</option>
                                                                        <% } else { %>
                                                                            <option value="TIE">TIE</option>
                                                                        <% } %>
                                                                        <% if(deviceData[0].devApp == "DIST") { %>
                                                                            <option value="DIST" selected="selected">DIST</option>
                                                                        <% } else { %>
                                                                            <option value="DIST">DIST</option>
                                                                        <% } %>
                                                                    </select>
                                                                </div>
                                                            </div>
                                                        </div>

                                                        <div class="row">
                                                            <div class="col-sm-4">
                                                                <div class="form-group">
                                                                    <label class="control-label">Select the CB Manufacturer/Platform</label>
                                                                    <select class="form-control select2" name="devMfgProd">
                                                                        <option>Select</option>
                                                                        <optgroup label="Schneider/Square D">
                                                                            <% if (deviceData[0].devProdLine == "Masterpact NW") { %>
                                                                                <option value="Square D/Masterpact NW(ICCB)" selected>Masterpact NW (ICCB)</option>
                                                                            <% } else { %>
                                                                                <option value="Square D/Masterpact NW(ICCB)">Masterpact NW (ICCB)</option>
                                                                            <% } %>
                                                                            <% if (deviceData[0].devProdLine == "Powerpact") { %>
                                                                                <option value="Square D/Powerpact(MCCB)" selected>Powerpact (MCCB) </option>
                                                                            <% } else { %>
                                                                                <option value="Square D/Powerpact(MCCB)">Powerpact (MCCB) </option>
                                                                            <% } %>
                                                                        </optgroup>
                                                                        <optgroup label="Eaton">
                                                                            <% if (deviceData[0].devProdLine == "Magnum DS") { %>
                                                                                <option value="Eaton/Magnum DS(ICCB)" selected>Magnum DS (ICCB)</option>
                                                                            <% } else { %>
                                                                                <option value="Eaton/Magnum DS(ICCB)">Magnum DS (ICCB)</option>
                                                                            <% } %>
                                                                            <% if (deviceData[0].devProdLine == "NRX") { %>
                                                                                <option value="Eaton/NRX(ICCB)" selected>NRX (ICCB)</option>
                                                                            <% } else { %>
                                                                                <option value="Eaton/NRX(ICCB)">NRX (ICCB)</option>
                                                                            <% } %>
                                                                            <% if (deviceData[0].devProdLine == "Power Defense") { %>
                                                                                <option value="Eaton/Power Defense(MCCB)" selected>Power Defense (MCCB)</option>
                                                                            <% } else { %>
                                                                                <option value="Eaton/Power Defense(MCCB)">Power Defense (MCCB)</option>
                                                                            <% } %>
                                                                            <% if (deviceData[0].devProdLine == "Series C") { %>
                                                                                <option value="Eaton/Series C(MCCB)" selected>Series C (MCCB)</option>
                                                                            <% } else { %>
                                                                                <option value="Eaton/Series C(MCCB)">Series C (MCCB)</option>
                                                                            <% } %>
                                                                        </optgroup>
                                                                        <optgroup label="ABB">
                                                                            <% if (deviceData[0].devProdLine == "Emax 2") { %>
                                                                                <option value="ABB/Emax 2(ICCB)" selected>Emax 2 (ICCB)</option>
                                                                            <% } else { %>
                                                                                <option value="ABB/Emax 2(ICCB)">Emax 2 (ICCB)</option>
                                                                            <% } %>
                                                                            <% if (deviceData[0].devProdLine == "Tmax") { %>
                                                                                <option value="ABB/Tmax(MCCB)" selected>Tmax (MCCB)</option>
                                                                            <% } else { %>
                                                                                <option value="ABB/Tmax(MCCB)">Tmax (MCCB)</option>
                                                                            <% } %>
                                                                        </optgroup>
                                                                        <optgroup label="Siemens">
                                                                            <% if (deviceData[0].devProdLine == "WL") { %>
                                                                                <option value="Siemens/WL(ICCB)" selected>WL (ICCB)</option>
                                                                            <% } else { %>
                                                                                <option value="Siemens/WL(ICCB)">WL (ICCB)</option>
                                                                            <% } %>
                                                                            <% if (deviceData[0].devProdLine == "VL") { %>
                                                                                <option value="Siemens/VL(MCCB)" selected>VL (MCCB)</option>
                                                                            <% } else { %>
                                                                                <option value="Siemens/VL(MCCB)">VL (MCCB)</option>
                                                                            <% } %>
                                                                        </optgroup>
                                                                        <optgroup label="LSIS">
                                                                            <% if (deviceData[0].devProdLine == "Susol_ICCB") { %>
                                                                                <option value="LSIS/Susol(ICCB)" selected>Susol (ICCB)</option>
                                                                            <% } else { %>
                                                                                <option value="LSIS/Susol(ICCB)">Susol (ICCB)</option>
                                                                            <% } %>
                                                                            <% if (deviceData[0].devProdLine == "Susol_MCCB") { %>
                                                                                <option value="LSIS/Susol(MCCB)" selected>Susol (MCCB)</option>
                                                                            <% } else { %>
                                                                                <option value="LSIS/Susol(MCCB)">Susol (MCCB)</option>
                                                                            <% } %>
                                                                        </optgroup>
                                                                    </select>
                                                                </div>
                                                            </div>
                                                            <div class="col-sm-4">
                                                                <div class="form-group">
                                                                    <label for="example-text-input" class="col-form-label">Breaker P/N</label>
                                                                    <input type="text" name="brkPN" class="form-control" value="<%= deviceData[0].brkPN%>" placeholder="Type Breaker P/N Here" autocomplete="off" required/>
                                                                </div>
                                                            </div>
                                                            <div class="col-sm-4">
                                                                <div class="form-group">
                                                                    <label for="example-text-input" class="col-form-label">Cradle P/N</label>
                                                                    <input type="text" name="cradlePN" class="form-control" value="<%= deviceData[0].cradlePN%>" placeholder="Type Cradle P/N Here" autocomplete="off"/>
                                                                </div>
                                                            </div>
                                                        </div>



                                                        <div class="row">
                                                            <div class="col-sm-2">
                                                                <div class="form-group">
                                                                    <label class="control-label">UL Rating</label>
                                                                    <select class="form-control" name="devUL">
                                                                        <option>Select</option>
                                                                        <% if (deviceData[0].devUL == "UL1066") { %>
                                                                            <option value="UL1066" selected>UL1066</option>
                                                                        <% } else { %>
                                                                            <option value="UL1066">UL1066</option>
                                                                        <% } %>
                                                                        <% if (deviceData[0].devUL == "UL489") { %>
                                                                            <option value="UL489" selected>UL489</option>
                                                                        <% } else { %>
                                                                            <option value="UL489">UL489</option>
                                                                        <% } %>
                                                                    </select>
                                                                </div>
                                                            </div>
                                                            <div class="col-sm-2">
                                                                <div class="form-group">
                                                                    <label class="control-label">Device Mounting</label>
                                                                    <select class="form-control" name="devMount">
                                                                        <option>Select</option>
                                                                        <% if (deviceData[0].devMount == "DRAW-OUT") { %>
                                                                            <option value="DRAW-OUT" selected>Draw-Out</option>
                                                                        <% } else { %>
                                                                            <option value="DRAW-OUT">Draw-Out</option>
                                                                        <% } %>
                                                                        <% if (deviceData[0].devMount == "FIXED") { %>
                                                                            <option value="FIXED" selected>Fixed</option>
                                                                        <% } else { %>
                                                                            <option value="FIXED">Fixed</option>
                                                                        <% } %>
                                                                        <% if (deviceData[0].devMount == "PLUG-IN") { %>
                                                                            <option value="PLUG-IN" selected>Plug-in</option>
                                                                        <% } else { %>
                                                                            <option value="PLUG-IN">Plug-in</option>
                                                                        <% } %>
                                                                    </select>
                                                                </div>
                                                            </div>
                                                            <div class="col-sm-2">
                                                                <div class="form-group">
                                                                    <label class="control-label">Poles</label>
                                                                    <select class="form-control" name="devPoles">
                                                                        <option>Select</option>
                                                                        <% if (deviceData[0].devPoles == "2") { %>
                                                                            <option value="2" selected>2</option>
                                                                        <% } else { %>
                                                                            <option value="2">2</option>
                                                                        <% } %>
                                                                        <% if (deviceData[0].devPoles == "3") { %>
                                                                            <option value="3" selected>3</option>
                                                                        <% } else { %>
