    <div class="content-page">
        <!-- Content Start -->

        <div class="content">
            <div class="container-fluid">

                <div class="row">
                    <div class="col-sm-12">
                        <div class="page-title-box">
                            <h4 class="page-title">Applications Engineering</h4>
                            <div class="row justify-content-center">
                                <div class="col-sm-5">
                                    <div class="card m-b-20" style="height:94%;">
                                        <div class="card-body">
                                            <h4 class="mt-0 header-title">Create Quote</h4>
                                            <h6 style="color:red;"><%= message %></h6>
                                            <form id="quoteForm" class="quoteForm" action="/addQuote" method="POST">
                                                <div class="form-group">
                                                    <div class="row">
                                                        <div class="col-sm-12">
                                                            <div class="form-group">
                                                                <label for="example-text-input" class="col-form-label">Quote Number</label>
                                                                <input type="text" name="quoteNum" class="form-control" value="" placeholder="Type Quote Number Here" id="example-text-input" autocomplete="off">
                                                            </div>
                                                        </div>
                                                    </div>
                                                    <div class="row">
                                                        <div class="col-sm-12">
                                                            <div class="form-group">
                                                                <label for="example-text-input" class="col-form-label">Project Name</label>
                                                                <input type="text" name="projName" class="form-control" value="" placeholder="Type Project Name Here" id="example-text-input" autocomplete="off">
                                                            </div>
                                                        </div>
                                                    </div>
                                                    <div class="row">
                                                        <div class="col-sm-6">
                                                            <div class="form-group">
                                                                <label for="example-text-input" class="col-form-label">Customer</label>
                                                                <input type="text" name="customer" class="form-control" value="" placeholder="Type Customer Name Here" id="example-text-input" autocomplete="off">
                                                            </div>
                                                        </div>
                                                        <div class="col-sm-6">
                                                            <div class="form-group">
                                                                <label for="example-text-input" class="col-form-label">Rep</label>
                                                                <input type="text" name="rep" class="form-control" value="" placeholder="Type Rep Name Here" id="example-text-input" autocomplete="off">
                                                            </div>
                                                        </div>
                                                    </div>
                                                    <div class="row">
                                                        <div class="col-sm-12">
                                                            <div class="form-group">
                                                                <label for="example-text-input" class="col-form-label">Quote Notes</label>
                                                                <textarea type="text" name="quoteNotes" class="form-control" value="" placeholder="Type Quote Notes Here" id="example-text-input" autocomplete="off"></textarea>
                                                            </div>
                                                        </div>
                                                    </div>
                                                </div>
                                                <div class="form-group">
                                                    <div class="row justify-content-center">
                                                        <div class="col-4">
                                                            <input id="uploadBtn" type="submit" value="Create New" class="btn-block btn-lg btn-outline-success waves-effect waves-light" />
                                                        </div>
                                                        <!-- COPY QUOTE -->
                                                        <div class="col-4">
                                                            <input id="copyBtn" type="button" value="Copy From Existing" class="btn-block btn-lg btn-outline-secondary waves-effect waves-light" data-toggle="modal" data-target="#copyQuoteModal"/>
                                                            <div class="modal fade" id="copyQuoteModal" tabindex="-1" role="dialog" aria-labelledby="copyQuoteModalTitle" aria-hidden="true">
                                                                <div class="modal-dialog modal-dialog-centered" role="document">
                                                                    <div class="modal-content">
                                                                        <div class="modal-header">
                                                                            <h5 class="modal-title" id="exampleModalLongTitle">Quote Search</h5>
                                                                            <button type="button" class="close" data-dismiss="modal" aria-label="Close">
                                                                                <span aria-hidden="true">&times;</span>
                                                                            </button>
                                                                        </div>
                                                                        <div class="modal-body">
                                                                            <form id="copyQuoteForm" class="copyQuoteForm" action="" method="POST">
                                                                                <div class="form-group">
                                                                                    <p class="mb-0" style="font-size: medium">
                                                                                        Please fill out the following and click "Copy" afterwards
                                                                                    </p>
                                                                                    <p class="text-muted m-b-5"></p>
                                                                                    <div class="row">
                                                                                        <div class="col-sm-12">
                                                                                            <label for="example-text-input" class="col-form-label">Quote Number</label>
                                                                                            <input type="text" name="quoteNum" class="form-control" value="" placeholder="Type Quote Number Here" id="example-text-input" autocomplete="off">
                                                                                        </div>
                                                                                    </div>
                                                                                </div>
                                                                            </form>
                                                                        </div>
                                                                        <div class="modal-footer justify-content-center">
                                                                            <div class="w-50">
                                                                                <input id="copyQuoteBtn" type="submit" value="Copy" class="btn-block btn-lg btn-info" onclick="" />
                                                                            </div>
                                                                        </div>
                                                                    </div>
                                                                </div>
                                                            </div>
                                                        </div>
                                                        <!-- END COPY QUOTE -->
                                                    </div>
                                                </div>
                                            </form>
                                        </div>
                                    </div>
                                </div>
                                <div class="col-sm-7">
                                    <div class="card m-b-20">
                                        <div class="card-body">
                                            <h4 class="mt-0 header-title">Lookup Quote</h4>
                                            <p></p>
                                            <div class="row">
                                                <div class="col-sm-12">
                                                    <table id="quoteSumTable" class="table table-bordered dt-responsive nowrap" style="border-collapse: collapse; border-spacing: 0; width: 100%; display: block; overflow: auto;">
                                                        <thead>
                                                        <tr class="header">
                                                            <th style="display:none;">quoteID</th>
                                                            <th style="width:10%;">Quote Number</th>
                                                            <th style="width:10%;">Project Name</th>
                                                            <th style="width:20%;">Customer</th>
                                                            <th style="width:20%;">Rep</th>
                                                            <th style="width:20%;">Quote Notes</th>
                                                            <th style="width:20%;">Created By</th>
                                                            <th style="width:10%;"></th>
                                                        </tr>
                                                        </thead>
                                                        <tbody>
                                                        <% if(quoteData){ %>
                                                            <% if(quoteData.length != 0){ %>
                                                                <% for(var i = 0; i < quoteData.length; i++){ %>
                                                                    <tr>
                                                                        <td style="display:none;"><%= quoteData[i].quoteID %></td>
                                                                        <td><%= quoteData[i].quoteNum %></td>
                                                                        <td><%= quoteData[i].projName %></td>
                                                                        <td><%= quoteData[i].customer %></td>
                                                                        <td><%= quoteData[i].rep %></td>
                                                                        <td><%= quoteData[i].quoteNotes %></td>
                                                                        <td><%= quoteData[i].createdBy %></td>
                                                                        <td><a href="./quoteDetail/?quoteID=<%= quoteData[i].quoteNum %>_<%= quoteData[i].revNum%>"><button id="openBtn" class="btn-block btn btn-outline-secondary waves-effect waves-light">Open</button></a></td>
                                                                    </tr>
                                                                <% } %>
                                                            <% } %>
                                                        <% } %>
                                                        </tbody>
                                                    </table>
                                                </div>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div> <!-- container-fluid -->
        </div> <!-- content -->
    </div> <!-- content-page -->
    <footer class="footer">
        © 2019 SAI, Advanced Power Solutions Inc.
    </footer>
</div> <!-- wrapper -->


