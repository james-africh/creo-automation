<div class="content">
    <div class="container-fluid">
        <div class="row">
            <div class="col-sm-12">
                <div class="page-title-box">
                    <h4 class="page-title">Applications Engineering</h4>
                    <div class="row justify-content-left">
                        <div class="col-sm-12">
                            <div class="card m-b-20">
                                <div class="card-body">
                                    <h4 class="mt-0 header-title">Current Quote</h4>
                                    <div class="form-group">
                                        <div class="row">
                                            <div class="col-sm-4">
                                                <label for="example-text-input" class="col-form-label">Quote Number</label>
                                                <% if (quoteData) { %>
                                                <input type="text" name="quoteNum" class="form-control" value="<%=quoteData[0].quoteNum%>" placeholder="Type Quote Number Here" id="example-text-input" disabled>
                                                <% } %>
                                            </div>
                                            <div class="col-sm-4">
                                                <label>Project Name</label>
                                                <% if (quoteData) { %>
                                                <input type="text" name="projName" class="form-control" value="<%=quoteData[0].projName%>" placeholder="Type Project Name Here" id="example-text-input" disabled>
                                                <% } %>
                                            </div>
                                            <div class="col-sm-4">
                                                <div class="form-group">
                                                    <label for="example-text-input" class="col-form-label">Customer</label>
                                                    <% if (quoteData) { %>
                                                    <input type="text" name="customer" class="form-control" value="<%=quoteData[0].customer%>" placeholder="Type Customer Name Here" id="example-text-input" disabled>
                                                    <% } %>
                                                </div>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>

                    <div class="row">
                        <div class="col-lg-12">
                            <div class="card m-b-20">
                                <div class="card-body">

                                    <h4 class="mt-0 header-title">Quote Data</h4>
                                    <!-- Nav tabs -->
                                    <ul class="nav nav-tabs nav-tabs-custom nav-justified" role="tablist">
                                        <li class="nav-item">
                                            <a class="nav-link disabled" data-toggle="tab" href="#lineup" role="tab">Lineups</a>
                                        </li>
                                        <li class="nav-item">
                                            <a class="nav-link disabled" data-toggle="tab" href="#devices" role="tab">Devices</a>
                                        </li>
                                        <li class="nav-item">
                                            <a class="nav-link active" data-toggle="tab" href="#sections" role="tab">Sections</a>
                                        </li>
                                        <li class="nav-item">
                                            <a class="nav-link disabled" data-toggle="tab" href="#hours" role="tab">Hours</a>
                                        </li>
                                    </ul>

                                    <!-- Tab panes -->
                                    <div class="tab-content">
                                        <div class="tab-pane p-3" id="lineup" role="tabpanel"></div>
                                        <div class="tab-pane p-3" id="devices" role="tabpanel"></div>
                                        <div class="tab-pane active p-3" id="sections" role="tabpanel">
                                            <div class="row">
                                                <div class="col-sm-2">
                                                    <label for="example-text-input" class="col-form-label"><h4></h4></label>
                                                    <% if (quoteData) { %>
                                                    <a href="../quoteDetail/?quote=<%=quoteData[0].quoteNum%>"><button class="btn-block btn-lg btn-outline-secondary waves-effect waves-light">< Back</button></a>
                                                    <% } %>
                                                </div>
                                            </div>
                                            <form id="LayoutDetailForm" class="LayoutDetailForm" action="" method="POST">
                                                <div class="row">
                                                    <div class="col-sm-2">
                                                        <div class="form-group">
                                                            <label for="example-text-input" class="col-form-label">Quote Number</label>
                                                            <% if (quoteData) { %>
                                                            <input type="text" name="quoteNum" class="form-control" value="<%=quoteData[0].quoteNum%>" placeholder="Type Quote Number Here" id="example-text-input" readonly="readonly">
                                                            <% } %>
                                                        </div>
                                                    </div>
                                                    <div class="col-sm-2">
                                                        <div class="form-group">
                                                            <label for="example-text-input" class="col-form-label">Layout</label>
                                                            <% if (layoutData) { %>
                                                            <input class="form-control" name="layoutNum" type="text" value="<%=layoutData[0].layoutNum %>" id="example-text-input" readonly="readonly">
                                                            <% } %>
                                                        </div>
                                                    </div>
                                                    <div class="col-sm-2">
                                                        <div class="form-group">
                                                            <label for="example-text-input" class="col-form-label">Layout Name</label>
                                                            <% if (layoutData) { %>
                                                            <input class="form-control" name="layoutName" type="text" value="<%=layoutData[0].layoutName %>" id="example-text-input" readonly="readonly">
                                                            <% } %>
                                                        </div>
                                                    </div>
                                                    <div class="col-sm-2">
                                                        <label for="example-text-input" class="col-form-label"><h4></h4></label>
                                                        <button id="addSectionBtn" type="submit" class="btn-block btn-lg btn-outline-info waves-effect waves-light add-button" onclick="this.form.action='/addSection'">Add Section</button>
                                                    </div>
                                                    <div class="col-sm-2">
                                                        <label for="example-text-input" class="col-form-label"><h4></h4></label>
                                                        <input id="uploadBtn" type="submit" value="Save Changes" class="btn-block btn-lg btn-outline-success waves-effect waves-light" onclick="this.form.action='/saveSectionChanges'"/>
                                                    </div>
                                                </div>
                                            </form>
                                            <form>
                                                <div class="row">
                                                    <div class="col-sm-2">
                                                        <h6>Circuit Breaker Queue</h6>
                                                        <ol data-draggable="queue" class="cbQueue">
                                                            <% if (deviceData) { %>
                                                                <% for (var i=0; i < deviceData.length; i++) { %>
                                                                    <li data-draggable="item"><%= deviceData[i].devDesignation %></li>
                                                                <% } %>
                                                            <% } %>
                                                        </ol>
                                                    </div>
                                                    <div class="col-sm-2">
                                                        <h6>Control Device Queue</h6>
                                                        <ol data-draggable="queue" class="controlQueue">
                                                            <li data-draggable="item">Item 1</li>
                                                            <li data-draggable="item">Item 2</li>
                                                            <li data-draggable="item">Item 3</li>
                                                            <li data-draggable="item">Item 4</li>
                                                        </ol>
                                                    </div>
                                                    <% if(layoutData) { %>
                                                        <% for (var i=0; i<layoutData[0].numSections; i++) { %>
                                                            <div class="col-sm-2">
                                                                <div class="row">
                                                                    <div class="col-sm-12">
                                                                        <h6>Section <%=i+1%></h6>
                                                                        <ol data-draggable="target">
                                                                        </ol>
                                                                    </div>
                                                                </div>
                                                                <div class="row">
                                                                    <div class="col-sm-6">
                                                                        <label>Height
                                                                            <input type="number" name="heightSec" class="form-control" value="" placeholder="#" id="example-text-input">
                                                                        </label>
                                                                    </div>
                                                                    <div class="col-sm-4">
                                                                        <div class="form-check">
                                                                            <p></p>
                                                                            <input type="checkbox" name="heightOverride" class="form-check-input" id="heightOverride" value="true"/>
                                                                            <label class="form-check-label" for="heightOverride">Override</label>
                                                                        </div>
                                                                    </div>
                                                                </div>
                                                                <div class="row">
                                                                    <div class="col-sm-6">
                                                                        <label>Width
                                                                            <input type="number" name="widthSec" class="form-control" value="" placeholder="#" id="example-text-input">
                                                                        </label>
                                                                    </div>
                                                                    <div class="col-sm-4">
                                                                        <div class="form-check">
                                                                            <p></p>
                                                                            <input type="checkbox" name="widthOverride" class="form-check-input" id="widthOverride" value="true"/>
                                                                            <label class="form-check-label" for="widthOverride">Override</label>
                                                                        </div>
                                                                    </div>
                                                                </div>
                                                                <div class="row">
                                                                    <div class="col-sm-6">
                                                                        <label>Depth
                                                                            <input type="number" name="depthSec" class="form-control" value="" placeholder="#" id="example-text-input">
                                                                        </label>
                                                                    </div>
                                                                    <div class="col-sm-4">
                                                                        <div class="form-check">
                                                                            <p></p>
                                                                            <input type="checkbox" name="depthOverride" class="form-check-input" id="depthOverride" value="true"/>
                                                                            <label class="form-check-label" for="depthOverride">Override</label>
                                                                        </div>
                                                                    </div>
                                                                </div>
                                                            </div>
                                                        <% } %>
                                                    <% } %>
                                                </div>
                                            </form>
                                        <div class="tab-pane p-3" id="hours" role="tabpanel"></div>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div> <!-- container-fluid -->
</div> <!-- content -->
</div> <!-- content-page -->
<footer class="footer">
