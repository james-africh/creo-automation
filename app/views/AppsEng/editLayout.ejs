<div class="content">
    <div class="container-fluid">
        <div class="row">
            <div class="col-sm-12">
                <div class="page-title-box">
                    <h4 class="page-title">Applications Engineering</h4>
                    <div class="row justify-content-left">
                        <div class="col-sm-12">
                            <div class="card m-b-20">
                                <div class="card-body">
                                    <h4 class="mt-0 header-title">Current Quote</h4>
                                    <form id="quoteForm" class="quoteForm" action="/addQuote" method="POST">
                                        <div class="form-group">
                                            <div class="row">
                                                <div class="col-sm-4">
                                                    <label for="example-text-input" class="col-form-label">Quote Number</label>
                                                    <% if (quoteData) { %>
                                                    <input type="text" name="quoteNum" class="form-control" value="<%=quoteData[0].quoteNum%>" placeholder="Type Quote Number Here" id="example-text-input">
                                                    <% } %>
                                                </div>
                                                <div class="col-sm-4">
                                                    <label>Project Name</label>
                                                    <% if (quoteData) { %>
                                                    <input type="text" name="projName" class="form-control" value="<%=quoteData[0].projName%>" placeholder="Type Project Name Here" id="example-text-input">
                                                    <% } %>
                                                </div>
                                                <div class="col-sm-4">
                                                    <div class="form-group">
                                                        <label for="example-text-input" class="col-form-label">Customer</label>
                                                        <% if (quoteData) { %>
                                                        <input type="text" name="customer" class="form-control" value="<%=quoteData[0].customer%>" placeholder="Type Customer Name Here" id="example-text-input">
                                                        <% } %>
                                                    </div>
                                                </div>
                                            </div>
                                        </div>
                                    </form>
                                </div>
                            </div>
                        </div>
                    </div>


                    <div class="row">
                        <div class="col-lg-12">
                            <div class="card m-b-20">
                                <div class="card-body">

                                    <h4 class="mt-0 header-title">Quote Data</h4>
                                    <!-- Nav tabs -->
                                    <ul class="nav nav-tabs nav-tabs-custom nav-justified" role="tablist">
                                        <li class="nav-item">
                                            <a class="nav-link active" data-toggle="tab" href="#lineup" role="tab">Lineups</a>
                                        </li>
                                        <li class="nav-item">
                                            <a class="nav-link disabled" data-toggle="tab" href="#devices" role="tab">Devices</a>
                                        </li>
                                        <li class="nav-item">
                                            <a class="nav-link disabled" data-toggle="tab" href="#sections" role="tab">Sections</a>
                                        </li>
                                        <li class="nav-item">
                                            <a class="nav-link disabled" data-toggle="tab" href="#hours" role="tab">Hours</a>
                                        </li>
                                    </ul>

                                    <!-- Tab panes -->
                                    <div class="tab-content">

                                        <div class="tab-pane  active p-3" id="lineup" role="tabpanel">
                                            <p class="mb-0">
                                                Fill out the following for each layout, and click "Submit" afterwards
                                            </p>
                                            <form id="layoutForm" class="layoutForm" action="/saveLayoutChanges" method="POST">
                                                <div class="row">
                                                    <div class="col-sm-1">
                                                        <div class="form-group">
                                                            <label for="example-text-input" class="col-form-label">Layout</label>
                                                            <% if (layoutData) { %>
                                                            <input class="form-control" name="layoutNum" type="text" value="<%=layoutData[0].layoutNum%>" id="example-text-input" readonly="readonly">
                                                            <% } %>
                                                        </div>
                                                    </div>
                                                    <div class="col-sm-2">
                                                        <div class="form-group">
                                                            <label for="example-text-input" class="col-form-label">Quote Number</label>
                                                            <% if (quoteData) { %>
                                                            <input type="text" name="quoteNum" class="form-control" value="<%=quoteData[0].quoteNum%>" placeholder="Type Quote Number Here" id="example-text-input" readonly="readonly">
                                                            <% } %>
                                                        </div>
                                                    </div>
                                                    <div class="col-sm-4">
                                                        <div class="form-group">
                                                            <label for="example-text-input" class="col-form-label">Layout Name</label>
                                                            <input class="form-control" name="layoutName" type="text" value="<%=layoutData[0].layoutName%>" placeholder="Type Name Here" id="example-text-input" autocomplete="off">
                                                        </div>
                                                    </div>
                                                    <div class="col-sm-3">
                                                        <div class="form-group">
                                                            <label for="example-text-input" class="col-form-label">Layout Type</label>
                                                            <select class="form-control" name="layoutType">
                                                                <option>Select</option>
                                                                <% if (layoutData[0].layoutType == "LV Panelboard") { %>
                                                                    <option value="LV Panelboard" selected >LV Panelboard</option>
                                                                <% } else { %>
                                                                    <option value="LV Panelboard">LV Panelboard</option>
                                                                <% } %>
                                                                <% if (layoutData[0].layoutType == "LV Switchboard") { %>
                                                                    <option value="LV Switchboard" selected>LV Switchboard</option>
                                                                <% } else { %>
                                                                    <option value="LV Switchboard">LV Switchboard</option>
                                                                <% } %>
                                                                <% if (layoutData[0].layoutType == "LV Switchgear") { %>
                                                                    <option value="LV Switchgear" selected>LV Switchgear</option>
                                                                <% } else { %>
                                                                    <option value="LV Switchgear">LV Switchgear</option>
                                                                <% } %>
                                                                <% if (layoutData[0].layoutType == "MV Switchgear - SAI") { %>
                                                                    <option value="MV Switchgear - SAI" selected>MV Switchgear - SAI</option>
                                                                <% } else { %>
                                                                    <option value="MV Switchgear - SAI">MV Switchgear - SAI</option>
                                                                <% } %>
                                                                <% if (layoutData[0].layoutType == "MV Switchgear - Buyout") { %>
                                                                    <option value="MV Switchgear - Buyout" selected>MV Switchgear - Buyout</option>
                                                                <% } else { %>
                                                                    <option value="MV Switchgear - Buyout">MV Switchgear - Buyout</option>
                                                                <% } %>
                                                                <% if (layoutData[0].layoutType == "Retrofit") { %>
                                                                    <option value="Retrofit" selected>Retrofit</option>
                                                                <% } else { %>
                                                                    <option value="Retrofit">Retrofit</option>
                                                                <% } %>
                                                                <% if (layoutData[0].layoutType == "Control Box") { %>
                                                                    <option value="Control Box" selected>Control Box</option>
                                                                <% } else { %>
                                                                    <option value="Control Box">Control Box</option>
                                                                <% } %>
                                                            </select>
                                                        </div>
                                                    </div>
                                                    <div class="col-sm-2">
                                                        <div class="form-group">
                                                            <label for="example-text-input" class="col-form-label">UL Listing</label>
                                                            <select class="form-control" name="layoutUL">
                                                                <option>Select</option>
                                                                <% if (layoutData[0].layoutUL == "UL 891") { %>
                                                                    <option value="UL 891" selected>UL 891</option>
                                                                <% } else { %>
                                                                    <option value="UL 891">UL 891</option>
                                                                <% } %>
                                                                <% if (layoutData[0].layoutUL == "UL 1558") { %>
                                                                    <option value="UL 1558" selected>UL 1558</option>
                                                                <% } else { %>
                                                                    <option value="UL 1558">UL 1558</option>
                                                                <% } %>
                                                                <% if (layoutData[0].layoutUL == "SAI MV UL") { %>
                                                                    <option value="SAI MV UL" selected>SAI MV UL</option>
                                                                <% } else { %>
                                                                    <option value="SAI MV UL">SAI MV UL</option>
                                                                <% } %>
                                                            </select>
                                                        </div>
                                                    </div>
                                                </div>
                                                <div class="row">
                                                    <div class="col-sm-2">
                                                        <div class="form-group">
                                                            <label for="example-text-input" class="col-form-label">System Type</label>
                                                            <select class="form-control" name="layoutSystem">
                                                                <option>Select</option>
                                                                <% if (layoutData[0].layoutSystem == "480Y/277V - 3PH 4W") { %>
                                                                    <option value="480Y/277V - 3PH 4W" selected>480Y/277V - 3PH 4W</option>
                                                                <% } else { %>
                                                                    <option value="480Y/277V - 3PH 4W">480Y/277V - 3PH 4W</option>
                                                                <% } %>
                                                                <% if (layoutData[0].layoutSystem == "480V - 3PH 3W") { %>
                                                                    <option value="480V - 3PH 3W" selected>480V - 3PH 3W</option>
                                                                <% } else { %>
                                                                    <option value="480V - 3PH 3W">480V - 3PH 3W</option>
                                                                <% } %>
                                                                <% if (layoutData[0].layoutSystem == "5kV - 3PH 3W") { %>
                                                                    <option value="5kV - 3PH 3W" selected>5kV - 3PH 3W</option>
                                                                <% } else { %>
                                                                    <option value="5kV - 3PH 3W">5kV - 3PH 3W</option>
                                                                <% } %>
                                                                <% if (layoutData[0].layoutSystem == "15kV - 3PH 3W") { %>
                                                                    <option value="15kV - 3PH 3W" selected>15kV - 3PH 3W</option>
                                                                <% } else { %>
                                                                    <option value="15kV - 3PH 3W">15kV - 3PH 3W</option>
                                                                <% } %>
                                                                <% if (layoutData[0].layoutSystem == "31.5kV - 3PH 3W") { %>
                                                                    <option value="31.5kV - 3PH 3W" selected>31.5kV - 3PH 3W</option>
                                                                <% } else { %>
                                                                    <option value="31.5kV - 3PH 3W">31.5kV - 3PH 3W</option>
                                                                <% } %>
                                                            </select>
                                                        </div>
                                                    </div>
                                                    <div class="col-sm-2">
                                                        <div class="form-group">
                                                            <label for="example-text-input" class="col-form-label">Enclosure</label>
                                                            <select class="form-control" name="layoutEnclosure">
                                                                <option>Select</option>
                                                                <% if (layoutData[0].layoutEnclosure == "NEMA 1") { %>
                                                                    <option value="NEMA 1" selected>NEMA 1</option>
                                                                <% } else { %>
                                                                    <option value="NEMA 1">NEMA 1</option>
                                                                <% } %>
                                                                <% if (layoutData[0].layoutEnclosure == "NEMA 3R") { %>
                                                                    <option value="NEMA 3R" selected>NEMA 3R</option>
                                                                <% } else { %>
                                                                    <option value="NEMA 3R">NEMA 3R</option>
                                                                <% } %>
                                                            </select>
                                                        </div>
                                                    </div>
                                                    <div class="col-sm-3">
                                                        <div class="form-group">
                                                            <label for="example-text-input" class="col-form-label">Accessibility</label>
                                                            <select class="form-control" name="layoutAccess">
                                                                <option>Select</option>
                                                                <% if (layoutData[0].layoutAccess == "Front/Rear Access") { %>
                                                                <option value="Front/Rear Access" selected>Front/Rear Access</option>
                                                                <% } else { %>
                                                                <option value="Front/Rear Access">Front/Rear Access</option>
                                                                <% } %>
                                                                <% if (layoutData[0].layoutAccess == "Front Access Only") { %>
                                                                <option value="Front Access Only" selected>Front Access Only</option>
                                                                <% } else { %>
                                                                <option value="Front Access Only">Front Access Only</option>
                                                                <% } %>
                                                            </select>
                                                        </div>
                                                    </div>
                                                    <div class="col-sm-2">
                                                        <div class="form-group">
                                                            <label for="example-text-input" class="col-form-label">Cable Entry</label>
                                                            <select class="form-control" name="layoutCabEntry">
                                                                <option>Select</option>
                                                                <% if (layoutData[0].layoutCabEntry == "Top") { %>
                                                                    <option value="Top" selected>Top</option>
                                                                <% } else { %>
                                                                    <option value="Top">Top</option>
                                                                <% } %>
                                                                <% if (layoutData[0].layoutCabEntry == "Bottom") { %>
                                                                    <option value="Bottom" selected>Bottom</option>
                                                                <% } else { %>
                                                                    <option value="Bottom">Bottom</option>
                                                                <% } %>
                                                                <% if (layoutData[0].layoutCabEntry == "Top or Bottom") { %>
                                                                    <option value="Top or Bottom" selected>Top or Bottom</option>
                                                                <% } else { %>
                                                                    <option value="Top or Bottom">Top or Bottom</option>
                                                                <% } %>
                                                                <% if (layoutData[0].layoutCabEntry == "Undefined") { %>
                                                                    <option value="Undefined" selected>Undefined</option>
                                                                <% } else { %>
                                                                    <option value="Undefined">Undefined</option>
                                                                <% } %>
                                                            </select>
                                                        </div>
                                                    </div>
                                                    <div class="col-sm-3">
                                                        <div class="form-group">
                                                            <label for="example-text-input" class="col-form-label">Paint</label>
                                                            <select class="form-control" name="layoutPaint">
                                                                <option>Select</option>
                                                                <% if (layoutData[0].layoutPaint == "ANSI 61 GREY") { %>
                                                                    <option value="ANSI 61 GREY" selected>ANSI 61 Grey</option>
                                                                <% } else { %>
                                                                    <option value="ANSI 61 GREY">ANSI 61 Grey</option>
                                                                <% } %>
                                                                <% if (layoutData[0].layoutPaint == "RAL 7035 LIGHT GREY") { %>
                                                                    <option value="RAL 7035 LIGHT GREY" selected>RAL 7035 Light Grey</option>
