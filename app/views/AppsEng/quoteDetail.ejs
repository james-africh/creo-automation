    <div class="content">
        <div class="container-fluid">
            <div class="row">
                <div class="col-sm-12">
                    <div class="page-title-box">
                        <h4 class="page-title">Applications Engineering</h4>
                        <div class="row justify-content-center">
                            <div class="col-sm-12">
                                <div class="card m-b-20">
                                    <div class="card-body">
                                        <h4 class="mt-0 header-title">Current Quote</h4>
                                        <form id="quoteForm" class="quoteForm" action="/addQuote" method="POST">
                                            <div class="form-group">
                                                <div class="row">
                                                    <div class="col-sm-2">
                                                        <label for="quoteNum" class="col-form-label">Quote Number</label>
                                                        <% if (quoteData) { %>
                                                        <input type="text" name="quoteNum" class="form-control" value="<%=quoteData[0].quoteNum%>" placeholder="Type Quote Number Here" id="quoteNum" readonly="readonly">
                                                        <% } %>
                                                    </div>
                                                    <div class="col-sm-2">
                                                        <label for="revNum" class="col-form-label">Revision Number</label>
                                                        <% if (quoteData) { %>
                                                        <input type="text" name="revNum" class="form-control" value="<%=quoteData[0].revNum%>" placeholder="Type Quote Number Here" id="revNum" readonly="readonly">
                                                        <% } %>
                                                    </div>
                                                    <div class="col-sm-3">
                                                        <label for="projName" class="col-form-label">Project Name</label>
                                                        <% if (quoteData) { %>
                                                        <input type="text" name="projName" class="form-control" value="<%=quoteData[0].projName%>" placeholder="Type Project Name Here" id="projName" readonly="readonly">
                                                        <% } %>
                                                    </div>
                                                    <div class="col-sm-2">
                                                        <label for="customer" class="col-form-label">Customer</label>
                                                        <% if (quoteData) { %>
                                                        <input type="text" name="customer" class="form-control" value="<%=quoteData[0].customer%>" placeholder="Type Customer Name Here" id="customer" readonly="readonly">
                                                        <% } %>
                                                    </div>
                                                    <div class="col-sm-3">
                                                        <label for="rep" class="col-form-label">Rep</label>
                                                        <% if (quoteData) { %>
                                                        <input type="text" name="rep" class="form-control" value="<%=quoteData[0].rep%>" placeholder="" id="rep" readonly="readonly">
                                                        <% } %>
                                                    </div>
                                                </div>
                                            </div>
                                        </form>
                                        <!-- EDIT CURRENT QUOTE -->
                                        <div class="row justify-content-center">
                                            <div class="col-2">
                                                <input id="editQuoteButton" type="button" value="Edit" class="btn-block btn-lg btn-outline-warning waves-effect waves-light" data-toggle="modal" data-target="#editQuoteModal"/>
                                                <div class="modal fade" id="editQuoteModal" tabindex="-1" role="dialog" aria-labelledby="editQuoteModalTitle" aria-hidden="true">
                                                    <div class="modal-dialog modal-dialog-centered" role="document">
                                                        <div class="modal-content">
                                                            <div class="modal-header">
                                                                <h5 class="modal-title" id="editQuoteModalTitle">Edit Quote</h5>
                                                                <button type="button" class="close" data-dismiss="modal" aria-label="Close">
                                                                    <span aria-hidden="true">&times;</span>
                                                                </button>
                                                            </div>
                                                            <div class="modal-body">
                                                                <form id="editQuoteForm" class="editQuoteForm" action="../editQuote/?quoteID=<%= quoteData[0].quoteNum%>_<%= quoteData[0].revNum%>" method="POST">
                                                                    <div class="form-group">
                                                                        <p class="mb-0">
                                                                            Please fill out the following and click "Save" afterwards
                                                                        </p>
                                                                        <p class="text-muted m-b-5"></p>
                                                                        <div class="row">
                                                                            <div class="col-sm-6" >
                                                                                <label for="example-text-input" class="col-form-label">Quote Number</label>
                                                                                <% if (quoteData) { %>
                                                                                <input type="text" name="quoteNum" class="form-control" value="<%=quoteData[0].quoteNum%>" placeholder="Type Quote Number Here" id="example-text-input" readonly="readonly">
                                                                                <% } %>
                                                                            </div>
                                                                            <div class="col-sm-6" >
                                                                                <label for="example-text-input" class="col-form-label">Revision Number</label>
                                                                                <% if (quoteData) { %>
                                                                                <input type="text" name="revNum" class="form-control" value="<%=quoteData[0].revNum%>" placeholder="Type Rev Number Here" id="example-text-input" readonly="readonly">
                                                                                <% } %>
                                                                            </div>
                                                                        </div>
                                                                        <div class="row">
                                                                            <div class="col-sm-12">
                                                                                <label for="example-text-input" class="col-form-label">Project Name</label>
                                                                                <% if (quoteData) { %>
                                                                                <input type="text" name="projName" class="form-control" value="<%=quoteData[0].projName%>" placeholder="Type Project Name Here" id="example-text-input" autocomplete="off">
                                                                                <% } %>
                                                                            </div>
                                                                        </div>
                                                                        <div class="row">
                                                                            <div class="col-sm-12">
                                                                                <label for="example-text-input" class="col-form-label">Customer</label>
                                                                                <% if (quoteData) { %>
                                                                                <input type="text" name="customer" class="form-control" value="<%=quoteData[0].customer%>" placeholder="Type Customer Name Here" id="example-text-input" autocomplete="off">
                                                                                <% } %>
                                                                            </div>
                                                                        </div>
                                                                        <div class="row">
                                                                            <div class="col-sm-12">
                                                                                <label for="example-text-input" class="col-form-label">Rep</label>
                                                                                <% if (quoteData) { %>
                                                                                <input type="text" name="rep" class="form-control" value="<%=quoteData[0].rep%>" placeholder="Type Rep Name Here" id="example-text-input" autocomplete="off">
                                                                                <% } %>
                                                                            </div>
                                                                        </div>
                                                                    </div>
                                                                    <div class="modal-footer justify-content-center">
                                                                        <div class="w-50">
                                                                            <% if(quoteData) { %>
                                                                                <input id="editQuoteModalButton" type="submit" value="Save" class="btn-block btn-lg btn-info" />
                                                                            <% } %>
                                                                        </div>
                                                                    </div>
                                                                </form>
                                                            </div>
                                                        </div>
                                                    </div>
                                                </div>
                                            </div>
                                        </div>
                                        <!-- END EDIT CURRENT QUOTE -->
                                    </div>
                                </div>
                            </div>
                        </div>


                        <!-- START QUOTE CONFIG -->
                        <div class="row justify-content-center" style="margin-bottom: -1.2rem;">
                            <div class="col-sm-12">
                                <div class="card m-b-20">
                                    <div class="card-body">
                                        <div class="row" style="padding-left: 1%;">
                                            <h4 class="mt-0 header-title">Quote Configure</h4>
                                            <!-- CONFIGURE QUOTE ADD -->
                                            <div class="dropdown">
                                                <!--<button class="btn dropdown-toggle" type="button" id="quoteConfigureAdd" data-toggle="dropdown" aria-haspopup="true" aria-expanded="false" style="background-image: linear-gradient(90deg, #00C0FF 0%, #FFCF00 49%, #FC4F4F 80%, #00C0FF 100%); margin: -.35rem 0 0 0.5rem;  font-size: 20px; padding : 0 .5rem 0 .5rem;">-->
                                                <button class="btn dropdown-toggle" type="button" id="quoteConfigureAdd" data-toggle="dropdown" aria-haspopup="true" aria-expanded="false" style="background-color: lightgrey; margin: -.35rem 0 0 0.5rem;  font-size: 20px; padding : 0 .5rem 0 .5rem;">
                                                    +
                                                </button>
                                                <div class="dropdown-menu" aria-labelledby="quoteConfigureAdd">
                                                    <div class="dropdown-item btn" data-toggle="modal" data-target="#addLayoutModal" style="font-size:16px;">Layout</div>
                                                    <div class="dropdown-item btn" data-toggle="modal" data-target="#addPartsLaborModal" style="font-size:16px;">Parts/Labor</div>
                                                    <div class="dropdown-item btn" data-toggle="modal" data-target="#addFieldServiceModal" style="font-size:16px;">Field Service</div>
                                                    <div class="dropdown-item btn" data-toggle="modal" data-target="#addFreightModal" style="font-size:16px;">Freight</div>
                                                    <div class="dropdown-item btn" data-toggle="modal" data-target="#addWarrantyModal" style="font-size:16px;">Warranty</div>
                                                </div>
                                            </div>
                                        </div>
                                        <!-- END CONFIGURE QUOTE ADD -->

                                        <!-- DISPLAYS IF QUOTE IS EMPTY -->
                                        <% if(layoutData.length == 0 && fieldServiceData.length == 0 && freightData.length == 0 && partsLaborData.length == 0) { %>
                                            <div class="row justify-content-center">
                                                <h6>Please select one of the following options</h6>
                                            </div>
                                            <!--<div class="row justify-content-center">
                                                    <h6>Ummm hey <%= quoteData[0].createdBy.split(' ')[0].charAt(0).toUpperCase() + quoteData[0].createdBy.split(' ')[0].slice(1).toLowerCase() %>...your quote doesn't have anything in it...</h6>
                                                </div>
                                                <div class="row justify-content-center">
                                                    <img src=" /public/uploads/meme.gif" alt="">
                                                </div>-->
                                            <div class="row justify-content-center">
                                                <div class="col-sm-2">
                                                    <label for="example-text-input" class="col-form-label"></label>
                                                    <input id="addLayoutBtn" type="button" value="Add Layout" class="btn-block btn-lg btn-outline-secondary waves-effect waves-light" data-toggle="modal" data-target="#addLayoutModal"/>
                                                </div>
                                                <div class="col-sm-2">
                                                    <label for="example-text-input" class="col-form-label"></label>
                                                    <input id="addPartsLaborBtn" type="button" value="Add Parts/Labor" class="btn-block btn-lg btn-outline-secondary waves-effect waves-light" data-toggle="modal" data-target="#addPartsLaborModal"/>
                                                </div>
                                                <div class="col-sm-2">
                                                    <label for="example-text-input" class="col-form-label"></label>
                                                    <input id="addFieldServiceBtn" type="button" value="Add Field Service" class="btn-block btn-lg btn-outline-secondary waves-effect waves-light" data-toggle="modal" data-target="#addFieldServiceModal"/>
                                                </div>
                                                <div class="col-sm-2">
                                                    <label for="example-text-input" class="col-form-label"></label>
                                                    <input id="addFreightBtn" type="button" value="Add Freight" class="btn-block btn-lg btn-outline-secondary waves-effect waves-light" data-toggle="modal" data-target="#addFreightModal"/>
                                                </div>
                                                <div class="col-sm-2">
                                                    <label for="example-text-input" class="col-form-label"></label>
                                                    <input id="addWarrantyBtn" type="button" value="Add Warranty" class="btn-block btn-lg btn-outline-secondary waves-effect waves-light" data-toggle="modal" data-target="#addWarrantyModal"/>
                                                </div>
                                            </div>
                                        <% } else {%>
                                            <!-- START CAROUSEL -->
                                            <div id="carouselExampleIndicators" class="carousel slide" data-interval="false">
                                                <!-- INDICATORS FOR CAROUSEL SLIDES-->
                                                <ol class="carousel-indicators" id="carouselList">
                                                    <!-- LAYOUT SLIDES -->
                                                    <%var slideNum1 = 0; %>
                                                    <% if(layoutData){ %>
                                                        <% for(let i = 0; i < layoutData.length; i++){ %>
                                                            <% slideNum1 = i; %>
                                                            <% if(layoutData[i].layoutNum == currentSlide){ %>
                                                            <li data-target="#carouselExampleIndicators" data-toggle="tooltip" data-placement="bottom" title="<%=layoutData[i].layoutName%>" data-slide-to="<%= slideNum1 %>" class="carouselIndicator active" id="<%= slideNum1 %>" style="filter: invert(100%);"></li>
                                                            <% } else { %>
                                                            <li data-target="#carouselExampleIndicators" data-toggle="tooltip" data-placement="bottom" title="<%=layoutData[i].layoutName%>" data-slide-to="<%= slideNum1 %>" class="carouselIndicator" id="<%= slideNum1 %>" style="filter: invert(100%);"></li>
                                                            <% } %>
                                                        <% } %>
                                                    <% } %>

                                                    <!-- PARTS AND LABOR SLIDES -->
                                                    <% if(partsLaborData){ %>
                                                        <% for(let i = 0; i < partsLaborData.length; i++){ %>
                                                            <% slideNum1 += 1; %>
                                                            <% if(partsLaborData[i].partsLaborNum == currentSlide){ %>
