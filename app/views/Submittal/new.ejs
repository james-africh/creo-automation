                                                        <select class="form-control" id="devCtrlVolt-<%=currentBrk%>" name="devCtrlVolt">
                                                            <% let devCtrlVolts = ["120VAC", "24VDC", "Mixed"]; %>
                                                            <option value="" id="NONE">Select</option>
                                                            <% let optsCtrlVolt = brkDropdownOpts.filter(e => e.type == 'devCtrlVolt'); %>
                                                            <% for (let opt of optsCtrlVolt) { %>
                                                                <% for (let platform of platformArray) { %>
