<div class="col-sm-3">
    <label for="iccbPlatform" class="col-form-label">ICCB Platform</label>
    <select class="form-control" name="iccbPlatform" id="iccbPlatform" disabled="disabled">
        <%   let iccbPlatforms = layoutDropdownData.filter(e => e.type === 'iccbPlatform') %>
        <% for (let iccbPlatform of iccbPlatforms) { %>
            <% if (layoutData[0].iccbPlatform == 'N/A') { %>
                <option value="N/A" selected="selected">N/A</option>
            <% } else if (layoutData[0].iccbPlatform == iccbPlatform.value) { %>
            <option value="<%=iccbPlatform.value%>" selected="selected"><%=iccbPlatform.value%></option>
            <% } else { %>
            <option value="<%=iccbPlatform.value%>"><%=iccbPlatform.value%></option>
            <% } %>
        <% } %>
    </select>
</div>
