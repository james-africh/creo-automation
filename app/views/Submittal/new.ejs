                                <div class="row justify-content-center">
                                    <div class="col-sm-9">
                                        <label class="control-label">Accessories (Select all that apply):</label>
                                        <div class="container" id="accContainer" style="height: 28.4rem; border-style: solid; border-color: #cccccc; border-width: 0.09rem; padding: 1rem;  overflow-y: scroll;">
                                            <ul id="brkAccTable" style="display: inline-block">
                                                <% let checkedAcc = []; %>
                                                <% for (let devAcc of brkAccData) { %>
                                                    <% if (devAcc.devID == deviceData[i].devID) { %>
                                                        <% checkedAcc.push({
                                                            name: devAcc.name,
                                                            opt: devAcc.opt
                                                        }); %>
                                                    <% } %>
                                                <% } %>
                                                <% for (let brkAcc of brkAccDropdownData) { %>
                                                    <% if (brkAcc.value.length != 0 && brkAcc.value[0] != null) { %>
                                                        <% if (checkedAcc.filter(e => e.name == brkAcc.name).length > 0) { %>
                                                        <li>
                                                            <input type="checkbox" class="form-check-input" id="<%=brkAcc.type%>" value="<%=brkAcc.name%>" checked="checked" style="position:relative; top: 2px;" onclick="accBrkEdit(this)">
                                                            <label class="form-check-label" for="<%=brkAcc.type%>"><%=brkAcc.name%></label>
                                                            <ul id="<%=brkAcc.type%>Opt_Edit" style="display: block;">
                                                                <% for (let brkAccOpt of brkAcc.value) { %>
                                                                    <% if (checkedAcc.filter(e => e.opt == brkAccOpt).length > 0) { %>
                                                                    <li>
                                                                        <input type="hidden" value="1" name="accOpts"><input type="checkbox" class="form-check-input" id="<%=brkAcc.type%>_Opt_<%brkAccOpt%>" style="position:relative; top: 2px;" checked="checked" onclick="this.previousSibling.value=1-this.previousSibling.value">
