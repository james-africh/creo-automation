                                                                        <% } else { %>
                                                                        <option value="<%=opt.value%>" id="<%=platform.shorthand%>" hidden="hidden"><%=opt.value%></option>
                                                                        <% } %>
                                                                    <% } %>
                                                                <% } %>
                                                            <% } %>
                                                        </select>
                                                    </div>
                                                </div>
