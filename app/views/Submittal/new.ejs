                                                                <% } %>
                                                            <% } %>
                                                        </select>
                                                    </div>
                                                </div>
                                                <div class="col-sm-3">
                                                    <div class="form-group">
                                                        <label for="" class="control-label">kAIC</label>
                                                        <select class="form-control" id="devKAIC-<%=currentBrk%>" name="devKAIC">
