<div class="col-sm-3">
    <label for="iccbPlatform" class="col-form-label">ICCB Platform</label>
    <select class="form-control" name="iccbPlatform" id="iccbPlatform">
        <option value="Select">Select</option>
        <%   let iccbPlatforms = layoutDropdownData.filter(e => e.type === 'iccbPlatform') %>
        <% for (let iccbPlatform of iccbPlatforms) { %>
        <option value="<%=iccbPlatform.value%>"><%=iccbPlatform.value%></option>
        <% } %>
    </select>
</div>
