
<!-- EDIT CURRENT BREAKER MODAL -->
<div class="row justify-content-center" style="padding-bottom:1%;">
    <div class="col-12">
        <div class="modal fade" id="editBreakerModal<%=currentBrk%>" tabindex="-1" role="dialog" aria-labelledby="editBreakerModalTitle<%=currentBrk%>" aria-hidden="true">
            <div class="modal-dialog modal-dialog-centered modal-lg" role="document">
                <div class="modal-content">
                    <div class="modal-header">
                        <h5 class="modal-title" id="editBreakerModalTitle<%=currentBrk%>">Edit Breaker</h5>
                        <button type="button" class="close" data-dismiss="modal" aria-label="Close">
                            <span aria-hidden="true">&times;</span>
                        </button>
                    </div>
                    <div class="modal-body">
                        <form id="editBreakerForm" class="editBreakerForm" action="../submittalEditDevice/?subID=<%= subData[0].jobNum%><%= subData[0].releaseNum%>_<%= subData[0].subID%>&layoutID=<%=deviceData[i].layoutID%>&devID=<%= currentBrk %>" method="POST">
                            <div class="form-group">
                                <p class="mb-0">
                                    Please fill out the following and click "Save" afterwards
                                </p>
                                <% let platformArray = [{
                                    name: 'SQUARE D MASTERPACT NW',
                                    shorthand: 'NW'
                                }, {
                                    name: 'SQUARE D POWERPACT',
                                    shorthand: 'POWERPACT'
                                }, {
                                    name: 'ABB EMAX2',
                                    shorthand: 'EMAX2'
                                }, {
                                    name: 'ABB TMAX',
                                    shorthand: 'TMAX'
                                }] %>
                                <div class="row">
                                    <div class="col-sm-12">
                                        <div class="card-body">
                                            <div class="row">
                                                <div class="col-sm-2">
                                                    <div class="form-group">
                                                        <label for="example-text-input" class="col-form-label">Release</label>
                                                        <% if (subData) { %>
                                                        <input type="text" name="jobRelease" class="form-control" value="<%=subData[0].jobNum%><%=subData[0].releaseNum%>" id="example-text-input" readonly="readonly">
                                                        <% } %>
                                                    </div>
                                                </div>
                                                <div class="col-sm-2">
                                                    <div class="form-group">
                                                        <label for="example-text-input" class="col-form-label">Rev #</label>
                                                        <% if (revData) { %>
                                                        <input type="text" name="revNum" class="form-control" value="<%=revData[0].revNum%>"  id="example-text-input" readonly="readonly">
                                                        <% } %>
                                                    </div>
                                                </div>
                                                <div class="col-sm-3"  style="margin-top: 0.5rem;">
                                                    <div class="form-group">
                                                        <label for="" class="control-label">Cat. Code</label>
                                                        <select class="form-control" id="" name="catCode">
                                                            <option>Select</option>
                                                            <% let catCodes = ["36-CB MC", "37-CB IC", "38-DISC SW"]; %>
                                                            <% for (let catCode of catCodes) { %>
                                                                <% if (deviceData[i].catCode == catCode) { %>
                                                                <option value="<%=catCode%>" selected="selected"><%=deviceData[i].catCode%></option>
                                                                <% } else { %>
                                                                <option value="<%=catCode%>"><%=catCode%></option>
                                                                <% } %>
                                                            <% } %>
                                                        </select>
                                                    </div>
                                                </div>
                                                <div class="col-sm-5">
                                                    <div class="form-group">
                                                        <label for="example-text-input" class="col-form-label">Device Designation<i style="color: red;">&nbsp;*</i></label>
                                                        <input type="text" name="devDesignation" class="form-control" value="<%= deviceData[i].devDesignation%>" data-toggle="tooltip" data-placement="top" title="To declare multiple breakers (i.e. CB-1A, CB-2A, ..., CB-6A), use the following convention: CB-(1-6)A" placeholder="Type Designation Here" autocomplete="off" required/>
                                                    </div>
                                                </div>
                                            </div>
                                            <div class="row">
                                                <div class="col-sm-5">
                                                    <div class="form-group">
                                                        <label for="" class="col-form-label">Breaker P/N</label>
                                                        <input type="text" id="" name="brkPN" class="form-control" value="<%= deviceData[i].brkPN%>" placeholder="Type Breaker P/N Here" autocomplete="off"/>
                                                    </div>
                                                </div>
                                                <div class="col-sm-4">
                                                    <div class="form-group">
                                                        <label for="" class="col-form-label">Cradle P/N</label>
                                                        <input type="text" id="" name="cradlePN" class="form-control" value="<%= deviceData[i].cradlePN%>" placeholder="Type Cradle P/N Here" autocomplete="off"/>
                                                    </div>
                                                </div>
                                                <div class="col-sm-3" style="margin-top: 0.5rem;">
                                                    <div class="form-group">
                                                        <label for="" class="control-label">Function - Connection</label>
                                                        <select class="form-control" id="" name="devFunction">
                                                            <option>Select</option>
                                                            <% let devFunctions = ["MAIN - LL", "MAIN - BD", "MAIN - XFMR", "DIST - LL", "DIST - BD", "DIST - XFMR", "TIE - L", "TIE - R"]; %>
                                                            <% for (let devFunction of devFunctions) { %>
                                                                <% if (deviceData[i].devFunction == devFunction) { %>
                                                                <option value="<%=devFunction%>" selected="selected"><%=devFunction%></option>
                                                                <% } else { %>
                                                                <option value="<%=devFunction%>"><%=devFunction%></option>
                                                                <% } %>
                                                            <% } %>
                                                        </select>
                                                    </div>
                                                </div>
                                            </div>
                                            <div class="row">
                                                <div class="col-sm-4">
                                                    <label for="iccbPlatform" class="col-form-label">ICCB Platform</label>
                                                    <select class="form-control" name="iccbPlatform" id="iccbPlatform-<%=currentBrk%>" onchange="getBreakerPlatformEdit(this)">
                                                        <option value="">Select</option>
                                                        <% let iccbPlatforms = layoutDropdownData.filter(e => e.type === 'iccbPlatform') %>
                                                        <% for (let iccbPlatform of iccbPlatforms) { %>
                                                            <% if (deviceData[i].platform == iccbPlatform.value) { %>
                                                            <option value="<%=iccbPlatform.value%>" selected="selected"><%=iccbPlatform.value%></option>
                                                            <% } else { %>
                                                            <option value="<%=iccbPlatform.value%>"><%=iccbPlatform.value%></option>
                                                            <% } %>
                                                        <% } %>
                                                    </select>
                                                </div>
                                                <div class="col-sm-4">
                                                    <label for="mccbPlatform" class="col-form-label">MCCB Platform</label>
                                                    <select class="form-control" name="mccbPlatform" id="mccbPlatform-<%=currentBrk%>" onchange="getBreakerPlatformEdit(this)">
                                                        <option value="">Select</option>
                                                        <% let mccbPlatforms = layoutDropdownData.filter(e => e.type === 'mccbPlatform') %>
                                                        <% for (let mccbPlatform of mccbPlatforms) { %>
                                                            <% if (deviceData[i].platform == mccbPlatform.value) { %>
                                                            <option value="<%=mccbPlatform.value%>" selected="selected"><%=mccbPlatform.value%></option>
                                                            <% } else { %>
                                                            <option value="<%=mccbPlatform.value%>"><%=mccbPlatform.value%></option>
                                                            <% } %>
                                                        <% } %>
                                                    </select>
                                                </div>
                                                <div class="col-sm-4">
                                                    <label for="vcbPlatform" class="col-form-label">MV VCB Platform</label>
                                                    <select class="form-control" name="vcbPlatform" id="vcbPlatform-<%=currentBrk%>" onchange="getBreakerPlatformEdit(this)">
                                                        <option value="">Select</option>
                                                        <%   let vcbPlatforms = layoutDropdownData.filter(e => e.type === 'vcbPlatform') %>
                                                        <% for (let vcbPlatform of vcbPlatforms) { %>
                                                            <% if (deviceData[i].platform == vcbPlatform.value) { %>
                                                            <option value="<%=vcbPlatform.value%>" selected="selected"><%=vcbPlatform.value%></option>
                                                            <% } else { %>
                                                            <option value="<%=vcbPlatform.value%>"><%=vcbPlatform.value%></option>
                                                            <% } %>
                                                        <% } %>
                                                    </select>
                                                </div>

                                            </div>
