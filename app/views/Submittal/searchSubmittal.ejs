<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8" />
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0, user-scalable=0, minimal-ui">
    <title><%= title %></title>
    <meta content="Quote Detail" name="SAI-APS-Portal" />
    <link rel="shortcut icon" href="/public/assets/images/SAI-Logo.JPG">
    <link rel="stylesheet" href="/public/plugins/morris/morris.css">

    <link href="/public/plugins/RWD-Table-Patterns/dist/css/rwd-table.min.css" rel="stylesheet" type="text/css" media="screen" />
    <link href="/public/plugins/bootstrap-colorpicker/css/bootstrap-colorpicker.min.css" rel="stylesheet" />
    <link href="/public/plugins/bootstrap-datepicker/dist/css/bootstrap-datepicker.min.css" rel="stylesheet" />
    <link href="/public/plugins/select2/css/select2.min.css" rel="stylesheet" type="text/css" />
    <link href="/public/plugins/bootstrap-touchspin/css/jquery.bootstrap-touchspin.min.css" rel="stylesheet" />

    <link href="/public/assets/css/quoteDragDrop.css" rel="stylesheet" type="text/css" />

    <!-- ION Slider -->
    <link href="/public/plugins/ion-rangeslider/ion.rangeSlider.css" rel="stylesheet" type="text/css"/>
    <link href="/public/plugins/ion-rangeslider/ion.rangeSlider.skinModern.css" rel="stylesheet" type="text/css"/>

    <link href="/public/assets/css/bootstrap.min.css" rel="stylesheet" type="text/css">
    <link href="/public/assets/css/metismenu.min.css" rel="stylesheet" type="text/css">
    <link href="/public/assets/css/icons.css" rel="stylesheet" type="text/css">
    <link href="/public/assets/css/quoteControlTable.css" rel="stylesheet" type="text/css">
    <link href="/public/assets/css/quoteBrkTable.css" rel="stylesheet" type="text/css">
    <link href="/public/assets/css/quoteDragDrop.css" rel="stylesheet" type="text/css">
    <link href="/public/assets/css/style.css" rel="stylesheet" type="text/css">

    <link href="https://cdn.datatables.net/1.10.20/css/dataTables.bootstrap.min.css" rel="stylesheet" type="text/css">
    <link href="https://cdn.datatables.net/1.10.20/css/jquery.dataTables.min.css" rel="stylesheet" type="text/css">

    <style>
        .icon-success {
            color: #00C851;
        }
        .icon-danger {
            color: #ff4444;
        }
    </style>
    <style>
        div{
            overflow: visible !important;
        }
        div#accContainer, div#pbConfig{
            overflow: auto !important;
        }

        .carousel-inner>.carousel-item {
            -webkit-transition: -webkit-transform 150ms ease-in-out !important;
            -o-transition: -o-transform 150ms ease-in-out !important;
            transition: transform 150ms ease-in-out !important;
        }

        .dropdown-toggle::after {
            content: none;
        }
    </style>

    <script src="/public/assets/js/jquery.min.js"></script>
    <script src="https://cdn.datatables.net/1.10.20/js/jquery.dataTables.min.js"></script>
    <script>
        $(document).ready(function() {
            $('#mbomSumTable').DataTable({
                "scrollCollapse": true,
                "paging": true,
                "pagingType": "simple"
            });
        });
    </script>

    <script>
        $(document).ready(function() {
            $('#mbomComItemTable').DataTable({
                "scrollCollapse": true,
                "paging": true,
                "pagingType": "simple"
            });
        });
    </script>

    <script>
        document.addEventListener("DOMContentLoaded", function(event) {
            var scrollpos = localStorage.getItem('scrollpos');
            if (scrollpos) window.scrollTo(0, scrollpos);
        });

        window.onbeforeunload = function(e) {
            localStorage.setItem('scrollpos', window.scrollY);
        };
    </script>
</head>

<body>
<div id="wrapper">
    <!-- Top Bar Start -->
    <div class="topbar">

        <!-- LOGO -->
        <div class="topbar-left" style="background-color: white;">
            <a href="" class="logo">
                        <span>
                            <img src="/public/assets/images/SAI-Logo.JPG" alt="" height="67">
                        </span>
                <i>
                    <img src="/public/assets/images/SAI-Logo.JPG" alt="" height="22">
                </i>
            </a>
        </div>

        <nav class="navbar-custom">
            <ul class="navbar-right d-flex list-inline float-right mb-0">
                <li class="dropdown notification-list d-none d-sm-block">
                    <form role="search" class="app-search">
                        <div class="form-group mb-0">
                            <input type="text" class="form-control" placeholder="Search..">
                            <button type="submit"><i class="fa fa-search"></i></button>
                        </div>
                    </form>
                </li>
            </ul>

            <ul class="list-inline menu-left mb-0">
                <li class="float-left">
                    <button class="button-menu-mobile open-left waves-effect">
                        <i class="mdi mdi-menu"></i>
                    </button>
                </li>
            </ul>

        </nav>

    </div>
    <!-- Top Bar End -->

    <!-- ========== Left Sidebar Start ========== -->
    <div class="left side-menu">
        <div class="slimscroll-menu" id="remove-scroll">

            <!--- Sidemenu -->
            <div id="sidebar-menu">
                <!-- Left Menu Start -->
                <ul class="metismenu" id="side-menu">
                    <li class="menu-title">CREO Automation</li>
                    <li>
                        <a href="../PDF-DXF-BIN_BOM" class="waves-effect">
                            <i class="mdi mdi-content-duplicate"></i> <span> PDF / DXF / BIN BOM </span>
                        </a>
                    </li>
                    <li>
                        <a href="../submittal" class="waves-effect">
                            <i class="mdi mdi-file-pdf"></i> <span> Submittal </span>
                        </a>
                    </li>
                </ul>

            </div>
            <!-- Sidebar -->
            <div class="clearfix"></div>

        </div>
        <!-- Sidebar -left -->

    </div>
    <!-- Left Sidebar End -->

    <div class="content-page">
        <!-- Content Start -->

        <div class="content">
            <div class="container-fluid">
                <div class="row">
                    <div class="col-sm-12">
                        <div class="page-title-box">
                            <h4 class="page-title">Current Submittal - Latest Rev</h4>
                            <div class="row justify-content-center">
                                <div class="col-sm-6">
                                    <div class="card m-b-20" style="height:94%;">
                                        <div class="card-body">
                                            <h4 class="mt-0 header-title">Project Data</h4>
                                            <h6 style="color:red;"><%= message %></h6>
                                            <% if  (subData.length != 0) { %>
                                                <% for (let i = 0; i < subData.length; i++) { %>
                                                <form id="submittalForm" class="submittalForm" action="" method="POST">
                                                    <div class="form-group">
                                                        <div class="row">
                                                            <div class="col-sm-3">
                                                                <label for="example-text-input" class="col-form-label">Job #</label>
                                                                <input type="text" name="jobNum" class="form-control" value="<%=subData[i].jobNum%>" placeholder="Type Number Here" id="example-text-input" autocomplete="off" readonly="readonly">
                                                            </div>
                                                            <div class="col-sm-2">
                                                                <label for="example-text-input" class="col-form-label">Release</label>
                                                                <input type="text" name="releaseNum" class="form-control" value="<%=subData[i].releaseNum%>" placeholder="Type Release Here" id="example-text-input" autocomplete="off" readonly="readonly">
                                                            </div>
                                                            <div class="col-sm-2">
                                                                <label for="example-text-input" class="col-form-label">Rev</label>
                                                                <input type="text" name="revNum" class="form-control" value="<%=revData[revData.length - 1].revNum%>" placeholder="Type Rev Here" id="example-text-input" autocomplete="off" readonly="readonly">
                                                            </div>
                                                            <div class="col-sm-5">
                                                                <label for="example-text-input" class="col-form-label">Customer</label>
                                                                <input type="text" name="customer" class="form-control" value="<%=subData[i].customer%>" placeholder="Type Customer Here" id="example-text-input" autocomplete="off" readonly="readonly">
                                                            </div>
                                                        </div>
                                                        <div class="row">
                                                            <div class="col-sm-12">
                                                                <label for="example-text-input" class="col-form-label">Job Name</label>
                                                                <input type="text" name="jobName" class="form-control" value="<%=subData[i].jobName%>" placeholder="Type Name Here" id="example-text-input" autocomplete="off" readonly="readonly">
                                                            </div>
                                                        </div>
                                                        <div class="row">
                                                            <div class="col-sm-12">
                                                                <label for="example-text-input" class="col-form-label">Layout Name</label>
                                                                <input type="text" name="layoutName" class="form-control" value="<%=subData[i].layoutName%>" placeholder="Type Designation Here" id="example-text-input" autocomplete="off" readonly="readonly">
                                                            </div>
                                                        </div>
                                                        <div class="row">
                                                            <div class="col-sm-2">
                                                                <label for="example-text-input" class="col-form-label">Drawn</label>
                                                                <input type="text" name="drawnBy" class="form-control" value="<%=subData[i].drawnBy%>" placeholder="Type Initials Here" id="example-text-input" autocomplete="off" readonly="readonly">
                                                            </div>
                                                            <div class="col-sm-4">
                                                                <label for="example-text-input" class="col-form-label">Drawn Date</label>
                                                                <input type="date" name="drawnDate" class="form-control" value="<%=subData[i].drawnDate%>" placeholder="Type Date Here" id="example-text-input" autocomplete="off" readonly="readonly">
                                                            </div>
                                                            <div class="col-sm-2">
                                                                <label for="example-text-input" class="col-form-label">Checked</label>
                                                                <input type="text" name="checkedBy" class="form-control" value="<%=subData[i].checkedBy%>" placeholder="Type Initials Here" id="example-text-input" autocomplete="off" readonly="readonly">
                                                            </div>
                                                            <div class="col-sm-4">
                                                                <label for="example-text-input" class="col-form-label">Checked Date</label>
                                                                <input type="date" name="checkedDate" class="form-control" value="<%=subData[i].checkedDate%>" placeholder="Type Date Here" id="example-text-input" autocomplete="off" readonly="readonly">
                                                            </div>
                                                        </div>
                                                        <div class="row">
                                                            <div class="col-sm-12">
                                                                <label for="example-text-input" class="col-form-label">Rev Note</label>
                                                                <input type="text" name="revNote" class="form-control" value="<%=revData[revData.length - 1].revNote%>" placeholder="Type Note Here" id="example-text-input" autocomplete="off" readonly="readonly">
                                                            </div>
                                                        </div>
                                                    </div>
                                                </form>

                                                <!-- EDIT CURRENT QUOTE -->
                                                <div class="row justify-content-center">
                                                    <div class="col-3">
                                                        <input id="editSubButton" type="button" value="Edit" class="btn-block btn btn-outline-warning waves-effect waves-light" data-toggle="modal" data-target="#editSubModal"/>
                                                        <div class="modal fade" id="editSubModal" tabindex="-1" role="dialog" aria-labelledby="editSubModalTitle" aria-hidden="true">
                                                            <div class="modal-dialog modal-dialog-centered modal-lg" role="document">
                                                                <div class="modal-content">
                                                                    <div class="modal-header">
                                                                        <h5 class="modal-title" id="editSubModalTitle">Edit Submittal</h5>
                                                                        <button type="button" class="close" data-dismiss="modal" aria-label="Close">
                                                                            <span aria-hidden="true">&times;</span>
                                                                        </button>
                                                                    </div>
                                                                    <div class="modal-body">
                                                                        <form id="editSubForm" class="editSubForm" action="../editSubmittal/?subID=<%= subData[0].jobNum%><%= subData[0].releaseNum%>_<%= subData[0].subID%>" method="POST">
                                                                            <div class="form-group">
                                                                                <p class="mb-0">
                                                                                    Please fill out the following and click "Save" afterwards
                                                                                </p>
                                                                                <p class="text-muted m-b-5"></p>

                                                                                <div class="row">
                                                                                    <div class="col-sm-2">
                                                                                        <label for="example-text-input" class="col-form-label">Job #</label>
                                                                                        <input type="text" name="jobNum" class="form-control" value="<%=subData[i].jobNum%>" placeholder="Type Number Here" id="example-text-input" autocomplete="off" readonly="readonly">
                                                                                    </div>
                                                                                    <div class="col-sm-2">
                                                                                        <label for="example-text-input" class="col-form-label">Release</label>
                                                                                        <input type="text" name="releaseNum" class="form-control" value="<%=subData[i].releaseNum%>" placeholder="Type Release Here" id="example-text-input" autocomplete="off" readonly="readonly">
                                                                                    </div>
                                                                                    <div class="col-sm-2">
                                                                                        <label for="example-text-input" class="col-form-label">Rev</label>
                                                                                        <input type="text" name="revNum" class="form-control" value="<%=revData[revData.length - 1].revNum%>" placeholder="Type Rev Here" id="example-text-input" autocomplete="off" readonly="readonly">
                                                                                    </div>
                                                                                    <div class="col-sm-6">
                                                                                        <label for="example-text-input" class="col-form-label">Customer</label>
                                                                                        <input type="text" name="customer" class="form-control" value="<%=subData[i].customer%>" placeholder="Type Customer Here" id="example-text-input" autocomplete="off">
                                                                                    </div>
                                                                                </div>
                                                                                <div class="row">
                                                                                    <div class="col-sm-12">
                                                                                        <label for="example-text-input" class="col-form-label">Job Name</label>
                                                                                        <input type="text" name="jobName" class="form-control" value="<%=subData[i].jobName%>" placeholder="Type Name Here" id="example-text-input" autocomplete="off">
                                                                                    </div>
                                                                                </div>
                                                                                <div class="row">
                                                                                    <div class="col-sm-12">
                                                                                        <label for="example-text-input" class="col-form-label">Layout Name</label>
                                                                                        <input type="text" name="layoutName" class="form-control" value="<%=subData[i].layoutName%>" placeholder="Type Designation Here" id="example-text-input" autocomplete="off">
                                                                                    </div>
                                                                                </div>
                                                                                <div class="row">
                                                                                    <div class="col-sm-2">
                                                                                        <label for="example-text-input" class="col-form-label">Drawn</label>
                                                                                        <input type="text" name="drawnBy" class="form-control" value="<%=subData[i].drawnBy%>" placeholder="Type Initials Here" id="example-text-input" autocomplete="off">
                                                                                    </div>
                                                                                    <div class="col-sm-4">
                                                                                        <label for="example-text-input" class="col-form-label">Drawn Date</label>
                                                                                        <input type="date" name="drawnDate" class="form-control" value="<%=subData[i].drawnDate%>" placeholder="Type Date Here" id="example-text-input" autocomplete="off">
                                                                                    </div>
                                                                                    <div class="col-sm-2">
                                                                                        <label for="example-text-input" class="col-form-label">Checked</label>
                                                                                        <input type="text" name="checkedBy" class="form-control" value="<%=subData[i].checkedBy%>" placeholder="Type Initials Here" id="example-text-input" autocomplete="off">
                                                                                    </div>
                                                                                    <div class="col-sm-4">
                                                                                        <label for="example-text-input" class="col-form-label">Checked Date</label>
                                                                                        <input type="date" name="checkedDate" class="form-control" value="<%=subData[i].checkedDate%>" placeholder="Type Date Here" id="example-text-input" autocomplete="off">
                                                                                    </div>
                                                                                </div>
                                                                                <div class="row">
                                                                                    <div class="col-sm-12">
                                                                                        <label for="example-text-input" class="col-form-label">Rev Note</label>
                                                                                        <input type="text" name="revNote" class="form-control" value="<%=revData[revData.length - 1].revNote%>" placeholder="Type Note Here" id="example-text-input" autocomplete="off" readonly="readonly">
                                                                                    </div>
                                                                                </div>
                                                                            </div>
                                                                            <div class="modal-footer justify-content-center">
                                                                                <div class="w-50">
                                                                                        <input id="editQuoteModalButton" type="submit" value="Save" class="btn-block btn btn-outline-info waves-effect waves-light" />
                                                                                </div>
                                                                            </div>
                                                                        </form>
                                                                    </div>
                                                                </div>
                                                            </div>
                                                        </div>
                                                    </div>
                                                    <div class="col-3">
                                                        <input id="revBtn" type="submit" value="Create Rev" class="btn-block btn btn-outline-secondary waves-effect waves-light" onclick="this.form.action='/createSubmittalRev'" />
                                                    </div>
                                                </div>
                                                <!-- END EDIT CURRENT SUBMITTAL -->
                                                <% } %>
                                            <% } %>
                                        </div>
                                    </div>
                                </div>

                                <div class="col-sm-6">
                                    <div class="card m-b-20" style="height:94%;">
                                        <div class="card-body">
                                            <h4 class="mt-0 header-title">Creo Data</h4>
                                            <h6 style="color:red;"><%= message %></h6>
                                            <% if  (creoData.length != 0) { %>
                                                <% for (let i = 0; i < creoData.length; i++) { %>
                                                <form id="creoDataForm" class="creoDataForm" action="" method="POST">
                                                    <div class="form-group">
                                                        <div class="row">
                                                            <div class="col-sm-6">
                                                                <label for="example-text-input" class="col-form-label">Layout Assembly</label>
                                                                <input type="text" name="layoutAsm" class="form-control" value="<%=creoData[i].layoutAsm%>" placeholder="Type Layout Assembly Here" id="example-text-input" autocomplete="off" readonly="readonly">
                                                            </div>
                                                            <div class="col-sm-6">
                                                                <label for="example-text-input" class="col-form-label">One Line Assembly</label>
                                                                <input type="text" name="oneLineAsm" class="form-control" value="<%=creoData[i].oneLineAsm%>" placeholder="Type One Line Assembly Here" id="example-text-input" autocomplete="off" readonly="readonly">
                                                            </div>
                                                        </div>
                                                        <div class="row">
                                                            <div class="col-sm-6">
                                                                <label for="example-text-input" class="col-form-label">Layout Drawing</label>
                                                                <input type="text" name="layoutDrw" class="form-control" value="<%=creoData[i].layoutDrw%>" placeholder="Type Layout Drawing Here" id="example-text-input" autocomplete="off" readonly="readonly">
                                                            </div>
                                                            <div class="col-sm-6">
                                                                <label for="example-text-input" class="col-form-label">Output Drawing</label>
                                                                <input type="text" name="outputPDF" class="form-control" value="<%=creoData[i].outputPDF%>" placeholder="Type Output Drawing Here" id="example-text-input" autocomplete="off" readonly="readonly">
                                                            </div>
                                                        </div>
                                                        <div class="row">
                                                            <div class="col-sm-12">
                                                                <label for="example-text-input" class="col-form-label">Working Directory</label>
                                                                <input type="text" name="workingDir" class="form-control" value="<%=creoData[i].workingDir%>" placeholder="Type Working Directory Here" id="example-text-input" autocomplete="off">
                                                            </div>
                                                        </div>
                                                        <div class="row">
                                                            <div class="col-sm-12">
                                                                <label for="example-text-input" class="col-form-label">Output Directory</label>
                                                                <input type="text" name="outputDir" class="form-control" value="<%=creoData[i].outputDir%>" placeholder="Type Output Directory Here" id="example-text-input" autocomplete="off">
                                                            </div>
                                                        </div>
                                                        <div class="row">
                                                            <div class="col-sm-12">
                                                                <label for="example-text-input" class="col-form-label">Start Directory</label>
                                                                <input type="text" name="startDir" class="form-control" value="<%=creoData[i].startDir%>" placeholder="Type Start Directory Here" id="example-text-input" autocomplete="off">
                                                            </div>
                                                        </div>
                                                    </div>
                                                    <div class="form-group">
                                                        <div class="row justify-content-center">
                                                            <div class="col-4">
                                                                <input id="setWDBtn" type="submit" value="Set Working Directory" class="btn-block btn btn-outline-secondary waves-effect waves-light" onclick="this.form.action='/setWD'" />
                                                            </div>
                                                        </div>
                                                    </div>
                                                </form>
                                                <% } %>
                                            <% } %>
                                        </div>
                                    </div>
                                </div>
                            </div>

                            <div class="row">
                                <div class="col-sm-12">
                                    <div class="card m-b-20" style="height:94%;">
                                        <div class="card-body">
                                            <h4 class="mt-0 header-title">Layout Data</h4>
                                            <h6 style="color:red;"><%= message %></h6>
                                            <% if (layoutData.length == 0) { %>
                                            <form id="layoutDataForm" class="layoutDataForm" action="/addLayout/?subID=<%=subData[0].jobNum%><%=subData[0].releaseNum%>_<%=subData[0].subID%>" method="POST">
                                                <div class="form-group">
                                                    <input type="text" value="<%=subData[0].layoutName%>" name="layoutName" hidden="hidden">
                                                    <div class="row">
                                                        <div class="col-sm-3">
                                                            <label for="ulListing" class="col-form-label">UL Listing</label>
                                                            <select class="form-control" name="ulListing" id="ulListing">
                                                                <option value="Select">Select</option>
                                                                <%   let ulListings = layoutDropdownData.filter(e => e.type === 'ulListing') %>
                                                                <% for (let ulListing of ulListings) { %>
                                                                <option value="<%=ulListing.value%>"><%=ulListing.value%></option>
                                                                <% } %>
                                                            </select>
                                                        </div>
                                                        <div class="col-sm-3">
                                                            <label for="systemType" class="col-form-label">System Type</label>
                                                            <select class="form-control" name="systemType" id="systemType">
                                                                <option value="Select">Select</option>
                                                                <%   let systemTypes = layoutDropdownData.filter(e => e.type === 'systemType') %>
                                                                <% for (let systemType of systemTypes) { %>
                                                                <option value="<%=systemType.value%>"><%=systemType.value%></option>
                                                                <% } %>
                                                            </select>
                                                        </div>
                                                        <div class="col-sm-3">
                                                            <label for="systemAmp" class="col-form-label">System Amp</label>
                                                            <select class="form-control" name="systemAmp" id="systemAmp">
                                                                <option value="Select">Select</option>
                                                                <%   let systemAmps = layoutDropdownData.filter(e => e.type === 'systemAmp') %>
                                                                <% for (let systemAmp of systemAmps) { %>
                                                                <option value="<%=systemAmp.value%>"><%=systemAmp.value%></option>
                                                                <% } %>
                                                            </select>
                                                        </div>
                                                        <div class="col-sm-3">
                                                            <label for="mainBusAmp" class="col-form-label">Main Bus Amp</label>
                                                            <select class="form-control" name="mainBusAmp" id="mainBusAmp">
                                                                <option value="Select">Select</option>
                                                                <%   let mainBusAmps = layoutDropdownData.filter(e => e.type === 'mainBusAmp') %>
                                                                <% for (let mainBusAmp of mainBusAmps) { %>
                                                                <option value="<%=mainBusAmp.value%>"><%=mainBusAmp.value%></option>
                                                                <% } %>
                                                            </select>
                                                        </div>
                                                    </div>
                                                    <div class="row justify-content-center">
                                                        <div class="col-sm-3">
                                                            <label for="enclosure" class="col-form-label">Enclosure</label>
                                                            <select class="form-control" name="enclosure" id="enclosure">
                                                                <option value="Select">Select</option>
                                                                <%   let enclosures = layoutDropdownData.filter(e => e.type === 'enclosure') %>
                                                                <% for (let enclosure of enclosures) { %>
                                                                <option value="<%=enclosure.value%>"><%=enclosure.value%></option>
                                                                <% } %>
                                                            </select>
                                                        </div>
                                                        <div class="col-sm-3">
                                                            <label for="accessibility" class="col-form-label">Accessibility</label>
                                                            <select class="form-control" name="accessibility" id="accessibility">
                                                                <option value="Select">Select</option>
